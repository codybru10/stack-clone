<%= render "layouts/errors", :object => @question %>

<%= form_for [current_user,@question] do |f| %>
<%= f.hidden_field :user_id, :value => @question.user_id %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <%= f.label :tags %>
  <%= f.text_field :tags %>
  <%= f.submit %>
<% end %>
